<div id="<%= dom_id livro %>">
  <p>
    <strong>Título:</strong>
    <%= livro.title %>
  </p>

  <p>
    <strong>Autor:</strong>
    <%= livro.author %>
  </p>

  <p>
    <strong>Nota:</strong>
    <%= livro.rating %>
  </p>

  <p>
    <strong>Descrição:</strong>
    <%= livro.description %>
  </p>

</div>
